{
  "namespace": "nutrition-tickets",
  "parametrised": true,
  "deployer": "built-in",
  "globals": {
    "staticOptions": {
      "maxAge": 3600000
    },
    "lang": "en",
    "moduleTitles": {},
    "explicitTopMenu": [],
    "plugins": {
      "actualAclProvider": {
        "module": "@iondv/acl/access/DsAcl",
        "initMethod": "init",
        "initLevel": 1,
        "options": {
          "dataSource": "ion://Db"
        }
      },
      "aclProvider": {
        "module": "@iondv/acl/access/aclMetaMap",
        "options": {
          "dataRepo": "ion://dataRepo",
          "acl": "ion://actualAclProvider",
          "accessManager": "ion://roleAccessManager",
          "calculator": "ion://calculator",
          "map": {
            "person@nutrition-tickets": {
              "isEntry": true,
              "sidAttribute": "user",
              "jumps": [
                "employee"
              ]
            },
            "employee@nutrition-tickets": {
              "sidAttribute": "id",
              "jumps": [
                "organization",
                "person",
                "substitution"
              ]
            }
          }
        }
      },
      "dataRepo": {
        "options": {
          "maxEagerDepth": 4
        }
      },
      "customProfile": {
        "module": "@iondv/commons/lib/CustomProfile",
        "options": {
          "metaRepo": "ion://metaRepo",
          "dataRepo": "ion://dataRepo",
          "fields": {
            "surname": {
              "caption": "Фамилия",
              "required": false,
              "readonly": true
            },
            "name": {
              "caption": "Имя",
              "required": false,
              "readonly": true
            },
            "patronymic": {
              "caption": "Отчество",
              "required": false,
              "readonly": true
            },
            "birthDate": {
              "caption": "Дата рождения",
              "required": false,
              "readonly": true,
              "type": 120
            },
            "createDatet": {
              "caption": "Метка времени создания",
              "required": false,
              "readonly": true,
              "type": 6
            },
            "modifeDate": {
              "caption": "Метка времени изменения",
              "required": false,
              "readonly": true,
              "type": 6
            },
            "creator": {
              "caption": "Создан пользователем",
              "required": false,
              "readonly": true
            },
            "editor": {
              "caption": "Изменен пользователем",
              "required": false,
              "readonly": true
            }
          },
          "propertyMap": {
            "person@nutrition-tickets": {
              "filter": "user",
              "properties": {
                "person": "id",
                "surname": "surname",
                "name": "name",
                "patronymic": "patronymic",
                "birthDate": "birthDate",
                "createDatet": "createDatet",
                "modifeDate": "modifeDate",
                "creator": "creator",
                "editor": "editor"
              }
            },
            "employee@nutrition-tickets": {
              "filter": "person.user",
              "recursion": {
                "id": "employee.id"
              },
              "properties": {
                "employee": "id"
              }
            }
          }
        }
      },
      "accounts": {
        "options": {
          "plugins": {
            "customProfile": "ion://customProfile"
          }
        }
      }
    },
    "jobs": {}
  },
  "modules": {
    "ionadmin": {
      "globals": null,
      "import": null,
      "statics": null
    },
    "registry": {
      "import": null,
      "globals": {
        "defaultPath": "",
        "inlineForm": true,
        "logo-2check": "common-static/logo.png",
        "statics-2check": {
          "common-static": "./templates/static"
        },
        "di": {
          "itemToExcel": {
            "module": "@iondv/registry/export/itemToExcel",
            "initMethod": "init",
            "initLevel": 0,
            "options": {
              "tplDir": "./export/item",
              "injectors": []
            }
          },
          "itemToDocx": {
            "module": "@iondv/registry/export/itemToDocx",
            "initMethod": "init",
            "initLevel": 0,
            "options": {
              "tplDir": "./export/item",
              "injectors": []
            }
          },
          "listToDocx": {
            "module": "@iondv/registry/export/listToDocx",
            "initMethod": "init",
            "initLevel": 0,
            "options": {
              "tplDir": "./export/list",
              "log": "ion://sysLog"
            }
          },
          "listToExcel": {
            "module": "@iondv/registry/export/listToExcel",
            "initMethod": "init",
            "initLevel": 0,
            "options": {
              "tplDir": "./export/list",
              "log": "ion://sysLog"
            }
          },
          "export": {
            "options": {
              "configs": {}
            }
          }
        }
      }
    },
    "rest": {
      "globals": {
        "di": {
          "crud": {
            "module": "@iondv/rest/lib/impl/crud",
			"options": {
              "auth": "ion://auth",
              "dataRepo": "ion://dataRepo"
            }
          }
        }
      }
    }
  }
}
